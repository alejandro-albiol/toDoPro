{
    "entryPoints": ["src"],
    "entryPointStrategy": "expand",
    "out": "docs",
    "name": "ToDoPro API Documentation",
    "excludePrivate": true,
    "excludeProtected": true,
    "excludeExternals": true,
    "hideGenerator": true,
    "categorizeByGroup": true,
    "categoryOrder": ["Services", "Controllers", "Models", "*"],
    "navigationLinks": {
        "GitHub": "https://github.com/alejandro-albiol/todopro",
        "API Docs": "/api-docs"
    },
    "skipErrorChecking": true,
    "tsconfig": "tsconfig.json"
} 